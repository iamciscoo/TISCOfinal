# Server-side Supabase (used by admin API routes)
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE=your_supabase_service_role_key  # WARNING: keep this secret, do not commit real value

# Client-side Supabase (optional, for any client usage)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Admin Authentication (server-only). Do NOT expose these publicly.
ADMIN_ACCESS_KEY=your_admin_access_key
ADMIN_SESSION_SECRET=generate_a_long_random_string
# Optional: restrict admin access by IP, comma-separated list (e.g., "127.0.0.1,10.0.0.1")
ADMIN_IP_ALLOWLIST=

# Clerk keys
# Get these from your Clerk dashboard -> API Keys
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key
